int "[1-9][0-9]*|0"
float "[0-9]+\\.[0-9]+"
string "\"[^\"]*\""
char "'[^']*'"
id "[a-zA-Z_][a-zA-Z0-9_]*"
op "[\\+\\-\\*/]"
bracket "\\(|\\)"
.ignore "[ \t\r\n]+"

---

<expr> ::= <casted> <op> <expr>
    | <casted>
    ;

<casted> ::= "(" <expr> ")"
    | <int>
    | <float>
    | <string>
    | <char>
    | <fun>
    ;

<fun> ::= <id> [ [ { <expr> "," } ] <expr> ] ;