int "[1-9][0-9]*|0"
float "[0-9]+\\.[0-9]+"
string "\"[^\"]*\""
char "'[^']*'"
id "[a-zA-Z_][a-zA-Z0-9_]*"
operator "[\\+-\\*/%&\\|^<>.Â°:!]+"
bracket "\\(|\\)"
comma ","
.ignore "[ \t\r\n]+"

---

<program> ::= { <fun> }
            | { <expr> }
            ;

<expr> ::= <casted> <operator> <expr>
         | <casted>
         ;

<instructions> ::= "(" { <expr> } ")"
                 | "(" { <fun> } ")"
                 ;

<casted> ::= "(" <expr> ")"
           | <int>
           | <float>
           | <string>
           | <char>
           | <instructions>
           ;

<fun> ::= <id> [ [ { <expr> "," } ] <expr> ] ;